<odoo>
    <!-- Paperformat Agreement -->
    <record id="report_paperformat_agreement" model="report.paperformat">
        <field name="name">Agreement F4</field>
        <field name="default" eval="True"/>
        <field name="format">Folio</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">17</field>
        <field name="margin_bottom">17</field>
        <field name="margin_left">17</field>
        <field name="margin_right">17</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">35</field>
        <field name="dpi">90</field>
    </record>
</odoo>
